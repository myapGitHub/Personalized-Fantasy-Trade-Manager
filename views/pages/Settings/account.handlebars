<div>
    <div id="userId-error">{{error}}</div>
    <form id="change-userId">
        <label for="userId">Change UserId</label>
        <input type="text" name="userId" id="userId" value="{{userId}}">
        <button type="submit">Submit</button>
    </form>
</div>

<div>
    <div id="benchMax-error">{{error}}</div>
    <form id="change-benchMax">
        <label for="benchMax">Change Bench Max</label>
        <input type="number" name="benchMax" id="benchMax" value="{{maxBench}}">
        <button type="submit">Submit</button>
    </form>
</div>

<div>
    <div id="squatMax-error">{{error}}</div>
