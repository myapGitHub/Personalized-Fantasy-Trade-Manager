<header>
    <nav>
        <a href="/friends/inbox">Inbox</a>
    </nav>
</header>



<div>{{error}}</div>

{{#if empty}}
<div>
    <h3>No Friends Found</h3>
</div>
{{else}}
    {{#if friendsArr}}
        <ul>
            {{#each friendsArr}}
                <li>
                    <a href="/search-user/{{this}}" data-id="{{this}}">{{this}}</a>
                </li>
            {{/each}}
        </ul>
    {{else}}
    <div>
        <h3>No Friends Found</h3>
    </div>
    {{/if}}
{{/if}}
